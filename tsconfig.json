{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "Node",
    "allowJs": true,
    "checkJs": true,
    "maxNodeModuleJsDepth": 3,
    "removeComments": true,
    "resolveJsonModule": true,
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "types": ["node", "cors", "express","compression","axios"]
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
