{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "Node",
    "allowJs": true,
    "checkJs": true,
    "maxNodeModuleJsDepth": 3,
    "removeComments": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "strict": true,
    "types": ["node", "cors", "express","compression","axios"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
